    "version": "0.2.0",
    "configurations": [
        {
        "name": "Python: Odoo13",
        "type": "python",
        "request": "launch",
        "stopOnEntry": false,
        "python": "D:\\Odoo 13.0\\python\\python.exe", //path of python in your local machine where odoo has installed
        "console": "integratedTerminal",
        "program": "${workspaceRoot}\\server\\odoo-bin",
        "args": [
            "--config=${workspaceRoot}\\server\\odoo.conf",
        ],
        "cwd": "${workspaceRoot}",
        "env": {},
        "envFile": "${workspaceRoot}/.env",
        "debugOptions": [
            "RedirectOutput"
        ]
    }]
}