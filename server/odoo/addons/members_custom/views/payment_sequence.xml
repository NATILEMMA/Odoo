<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_membership_payment" model="ir.sequence">
            <field name="name">Membership Payment</field>
            <field name="code">membership.payment</field>
            <field name="prefix">MBPY</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_supporter_payment" model="ir.sequence">
            <field name="name">Supporter Payment</field>
            <field name="code">supporter.members</field>
            <field name="prefix">SRPY</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_main_office_payment" model="ir.sequence">
           <field name="name">Main Office Payment</field>
           <field name="code">main.office.payment</field>
           <field name="prefix">Main Office/%(year)s/</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
            <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
        </record>

        <record id="donors_payment" model="ir.sequence">
           <field name="name">Donors Payment</field>
           <field name="code">donation.payment</field>
           <field name="prefix">Supports/%(year)s/</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
            <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
        </record>


        <record id="sequence_transfer_member" model="ir.sequence">
            <field name="name">Transfer Member</field>
            <field name="code">members.transfer</field>
            <field name="prefix">TRMB</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="sequence_training_leader" model="ir.sequence">
            <field name="name">Training Leaders</field>
            <field name="code">member.training</field>
            <field name="prefix">TRAIN</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="sequence_assembly" model="ir.sequence">
            <field name="name">General Assembly</field>
            <field name="code">assembly</field>
            <field name="prefix">Assembly</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_complaint" model="ir.sequence">
            <field name="name">Complaints</field>
            <field name="code">member.complaint</field>
            <field name="prefix">CMPT</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="get_member_id" model="ir.sequence">
            <field name="name">Member IDs</field>
            <field name="code">res.partner</field>
            <field name="prefix">AAPP</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>