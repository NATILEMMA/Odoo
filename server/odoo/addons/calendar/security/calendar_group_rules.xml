<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="module_category_calendars" model="ir.module.category">
        <field name="name">Calendar</field>
        
        <field name="description">Helps you manage your calendar...</field>
        <field name="sequence">1</field>
    </record>

    <record id="group_calendar_user" model="res.groups">
        <field name="name">User</field>
        <!-- <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/> -->
        <field name="category_id" ref="module_category_calendars"/>
    </record>

    <record id="group_calendar_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="module_category_calendars"/>
        <field name="implied_ids" eval="[(4, ref('group_calendar_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
   
    <record id="group_calander_group" model="res.groups">
        <field name="name">Calendar group</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>